CONFIG_FILE= /build/app.yaml

# API
HOST=127.0.0.1
PORT= 3000

#DB
DB_DRIVER_NAME=postgres
DB_DATA_SOURCE=postgres://rbo:rbo@localhost:5436/rbo?sslmode=disable
DB_MAX_OPEN_CONNS=10
DB_MAX_IDLE_CONNS=10
DB_CONN_MAX_LIFE_TIME_MS=36000000
